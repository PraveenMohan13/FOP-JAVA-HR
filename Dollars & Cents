import java.util.*;
public class Solution {

    public static void main(String[] args) {
        Scanner z=new Scanner(System.in);
        int d1,d2,c1,c2,c,d;
        d1=z.nextInt();
        c1=z.nextInt();
        d2=z.nextInt();
        c2=z.nextInt();
        c=c1+c2;
        d=d1+d2+c/100;
        c=c%100;
        System.out.println(d);
        System.out.println(c);
    }
}
